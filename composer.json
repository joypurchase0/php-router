{
  "name": "joypurchase0/router",
  "type": "library",
  "description": "Fork of Buki\\Router with PHP 8.2 compatibility fix",
  "keywords": [
    "router",
    "route",
    "routing",
    "php-router",
    "php8.2"
  ],
  "homepage": "https://github.com/joypurchase0/router",
  "license": "MIT",
  "authors": [
    {
      "name": "Joy Kalyan",
      "email": "joypurchase0@gmail.com",
      "homepage": "https://joykalyan.guru"
    },
    {
      "name": "İzni Burak Demirtaş (original author)",
      "email": "info@burakdemirtas.org",
      "homepage": "https://burakdemirtas.org"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "symfony/http-foundation": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.4"
  },
  "autoload": {
    "psr-4": {
      "Buki\\Router\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Buki\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "coverage": "vendor/bin/phpunit --coverage-html coverage",
    "dev": "cd tests/Example && php -S 127.0.0.1:8000 -t ./"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
